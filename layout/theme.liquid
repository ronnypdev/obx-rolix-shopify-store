<!doctype html>
<html>

<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }}
  <!-- Header hook for plugins -->
  <!-- {{'//cdn.jsdelivr.net/npm/@splidejs/splide@2.4.0/dist/css/splide-core.min.css'| stylesheet_tag }} -->
  {{ '//cdn.jsdelivr.net/npm/@splidejs/splide@2.4.0/dist/css/splide.min.css' | stylesheet_tag }}
  {{ 'application.css' | asset_url | stylesheet_tag }}
</head>

<body class="template-{{ template }}">

  {% render 'header' %}
  <main role="main">

    {{ content_for_layout }}
  </main>
  {% render 'footer' %}


  {{ '//cdn.jsdelivr.net/npm/@splidejs/splide@latest/dist/js/splide.min.js', | script_tag }}
  {{ 'application.js' | asset_url | script_tag }}
</body>

</html>